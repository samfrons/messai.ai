# ML Service Environment Variables

# Service Configuration
ML_SERVICE_NAME=messai-ml-engine
ML_VERSION=1.0.0
ML_DEBUG=false
ML_PORT=8001

# Database
ML_DATABASE_URL=postgresql://messai:messai_dev_password@localhost:5432/messai_dev

# Redis
ML_REDIS_URL=redis://localhost:6379

# CORS Origins (comma-separated)
ML_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# ML Model Settings
ML_MODEL_CACHE_TTL=3600
ML_MAX_BATCH_SIZE=100
ML_PREDICTION_TIMEOUT=30

# Monitoring
ML_ENABLE_METRICS=true
ML_LOG_LEVEL=INFO

# Materials Project API
ML_MATERIALS_PROJECT_API_KEY=your_api_key_here
ML_MATERIALS_CACHE_TTL=2592000