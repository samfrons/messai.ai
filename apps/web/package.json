{
  "name": "@messai/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 3000",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update-snapshots": "jest --updateSnapshot",
    "postinstall": "prisma generate --schema=../../prisma/schema.prisma",
    "prebuild": "prisma generate --schema=../../prisma/schema.prisma",
    "api:generate-sdk": "curl http://localhost:3000/api/v1/sdk > messai-sdk.ts",
    "api:docs": "open http://localhost:3000/api-docs",
    "workers": "tsx src/lib/jobs/start-workers.ts",
    "workers:dev": "tsx --watch src/lib/jobs/start-workers.ts",
    "jobs:init": "tsx -e \"import('./src/lib/jobs/init-scheduled-jobs').then(m => m.initializeScheduledJobs())\"",
    "jobs:list": "tsx -e \"import('./src/lib/jobs/init-scheduled-jobs').then(m => m.listScheduledJobs().then(console.log))\"",
    "jobs:clean": "tsx -e \"import('./src/lib/jobs/init-scheduled-jobs').then(m => m.removeAllScheduledJobs())\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@messai/ui": "workspace:*",
    "@prisma/client": "^6.11.1",
    "@react-three/drei": "^10.5.0",
    "@react-three/fiber": "^9.2.0",
    "@types/three": "^0.178.1",
    "bullmq": "^5.36.0",
    "dompurify": "^3.2.6",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.4.2",
    "lru-cache": "^11.1.0",
    "marked": "^16.1.1",
    "next": "~15.2.4",
    "next-auth": "^4.24.11",
    "next-swagger-doc": "^0.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^3.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.18.2",
    "three": "^0.178.0",
    "use-debounce": "^10.0.5",
    "uuid": "^11.0.6",
    "zod": "^4.0.5",
    "zod-to-openapi": "^0.1.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@mswjs/data": "^0.16.2",
    "@tanstack/react-query": "^5.83.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^10.0.0",
    "jest-mock-extended": "^4.0.0",
    "msw": "^2.10.4",
    "openapi-typescript-codegen": "^0.29.0",
    "prisma": "^6.11.1"
  }
}
